{
  "name": "sws",
  "version": "1.0.58",
  "description": "An Electron application with Vue and TypeScript",
  "license": "MIT",
  "main": "./out/main/index.js",
  "scripts": {
    "version:patch": "npm version patch --force --no-git-tag-version",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev",
    "build": "pnpm version:patch && pnpm build:win",
    "b": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run b && electron-builder --dir",
    "build:win": "chcp 65001 && pnpm run b && electron-builder --win --config",
    "build:mac": "pnpm run b && electron-builder --mac",
    "build:linux": "pnpm run b && electron-builder --linux",
    "ruoyi": "esno ./script/ruoyi.ts"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@imengyu/vue3-context-menu": "^1.3.7",
    "@types/big.js": "^6.2.2",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "big.js": "^6.2.1",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.7.8",
    "eslint-plugin-format": "^1.0.1",
    "file-saver": "^2.0.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "serialport": "^12.0.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/ep": "^1.1.14",
    "@iconify-json/material-symbols-light": "^1.1.11",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^18.19.9",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.11",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "chokidar": "^3.6.0",
    "electron": "24.8.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.8.27",
    "yargs": "^17.7.2"
  }
}
